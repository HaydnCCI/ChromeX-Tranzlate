{
    "name": "Tranzlate", 
    "version": "1.0.1", 
    "manifest_version": 2,
    "permissions": [ "tabs", "activeTab", "declarativeContent", "storage", "\u003Call_urls>" ],
    "web_accessible_resources": ["img/logo_small.png"], 
    "icons": {
        "128": "img/icon_128.png",
        "19": "img/icon_19.png",
        "38": "img/icon_38.png"
     },
    "content_scripts": [
        {
            "css": [ "css/content.min.css" ],
            "js": [ "js/content.js" ],
            "matches": [ "\u003Call_urls>" ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "Tranzlate",
        "default_popup": "popup/popup.html",
        "default_icon": {
            "19": "img/icon_19.png",
            "38": "img/icon_38.png"
         }
         
    },
    "background": {
        "scripts": ["background.js"]
    }
}